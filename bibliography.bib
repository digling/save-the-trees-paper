@Article{Liang2016,
  Title                               = {{S}pectral analysis of {C}hinese language: {C}o-occurrence networks from four literary genres},
  Author                              = {Wei Liang and Guanrong Chen},
  Doi                                 = {10.1016/j.physa.2015.12.121},
