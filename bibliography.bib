@Article{Chacon2015a,
  Title                               = {{I}mproved computational models of sound change shed light on the history of the {T}ukanoan languages},
  Author                              = {Chacon, Thiago Costa and List, Johann-Mattis},
  Journal                             = {Journal of Language Relationship},
  Number                              = {3},
  Pages                               = {177--204},
  Volume                              = {13},
