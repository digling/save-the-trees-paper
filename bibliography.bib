@Article{Pagel2006b,
  Title                               = {{Bayes}ian {A}nalysis of {C}orrelated {E}volution of {D}iscrete {C}haracters by {R}eversible-{J}ump {Markov} {C}hain {Monte Carlo}.},
  Author                              = {Mark Pagel and Andrew Meade},
  Doi                                 = {10.1086/503444},
  Journal                             = {The American Naturalist},
