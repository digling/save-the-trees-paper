@Article{Dawyndt2006,
  Title                               = {\{UPGMA\} clustering revisited: {A} weight-driven approach to transitive approximation},
  Author                              = {Peter Dawyndt and Hans De Meyer and Bernard De Baets},
  Doi                                 = {10.1016/j.ijar.2005.11.001},
