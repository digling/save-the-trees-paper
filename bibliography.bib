@Article{Newman2004a,
  Title                               = {{F}inding and evaluating community structure in networks},
  Author                              = {Newman, M. E. J. and Girvan, M.},
  Doi                                 = {10.1103/PhysRevE.69.026113},
  Journal                             = {Physical Review E},
  Number                              = {2},
