@Article{Nelson2004,
  Title                               = {{T}he {U}niversity of {S}outh {F}lorida free association, rhyme, and word fragment norms},
  Author                              = {Nelson, Douglas L. and McEvoy, Cathy and Schreiber, Thomas A.},
  Journal                             = {Behaviour Research Methods, Instruments, and Computers},
  Number                              = {4},
