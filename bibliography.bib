  Title                               = {{L}anguage contact and language change in the history of the {S}initic languages},
  Author                              = {LaPolla, Randy J.},
  Doi                                 = {10.1016/j.sbspro.2010.05.036},
  Journal                             = {Procedia, Social and Behavioral Sciences},
