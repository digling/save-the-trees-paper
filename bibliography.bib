  Title                               = {{C}omparison of directed and weighted co-occurrence networks of six languages},
  Author                              = {Yuyang Gao and Wei Liang and Yuming Shi and Qiuling Huang},
  Doi                                 = {10.1016/j.physa.2013.08.075},
