@Article{Pozdniakov2014,
  Title                               = {{O} poroge rodstva i indekse stabil'nosti v bazisnoj leksike pri massovom sravnenii: {A}tlanti{\v{c}}eskie jazyki},
  Author                              = {Pozdniakov, Konstantin},
  Journal                             = {Journal of Language Relationship},
  Pages                               = {187--237},
  Volume                              = {11},
  Year                                = {2014},
  Eprint                              = {http://jolr.ru/files/(144)jlr2014-11(187-237).pdf},
  Eprinttype                          = {PDF},

  Abstract                            = {The paper deals with certain key aspects of the procedure of establishing long-range genetic relationship between languages in the absence of a classic step-by-step reconstruction, such as: 1) the issue of establishing the randomness threshold for lexical matches on the Swadesh 100\-item wordlist, depending on the particularities of the employed algorithm; 2) the issue of stability ranking in the sphere of the basic lexicon; 3) the issue of multiple isolated roots that do not find any parallels in the other languages of the family. These general topics are discussed in the context of specific problems that arise during research on the genetic classification of Atlantic languages. The latter are traditionally considered to constitute a branch of the Niger-Congo macrofamily, but lexicostatistics suggests that their date of separation is no younger than 8000 . Two critical responses to the paper present further opinions on the difficult problematics of establishing long-range relationship; the author{\textquoteright}s final response is also to be regarded as an important constituent of the published paper. В статье рассматриваются не{\cyrchar\cyrk}оторые {\cyrchar\cyrk}лючевые проблемы установления дальнего родства язы{\cyrchar\cyrk}ов в условиях, {\cyrchar\cyrk}огда отсутствует {\cyrchar\cyrk}лассичес{\cyrchar\cyrk}ая ступенчатая ре{\cyrchar\cyrk}онстру{\cyrchar\cyrk}ция, а именно: 1) проблема установления порога случайных сходств в стословном спис{\cyrchar\cyrk}е Сводеша (при различных алгоритмах их фи{\cyrchar\cyrk}сации); 2) проблема ранжирования базисной ле{\cyrchar\cyrk}си{\cyrchar\cyrk}и по ее устойчивости; 3) проблема многочисленных изолированных {\cyrchar\cyrk}орней, не находящих параллелей в других язы{\cyrchar\cyrk}ах семьи. Эти общие сюжеты рассматриваются на основе {\cyrchar\cyrk}он{\cyrchar\cyrk}ретной проблемати{\cyrchar\cyrk}и построения генетичес{\cyrchar\cyrk}ой {\cyrchar\cyrk}лассифи{\cyrchar\cyrk}ации атлантичес{\cyrchar\cyrk}их язы{\cyrchar\cyrk}ов. Атлантичес{\cyrchar\cyrk}ие язы{\cyrchar\cyrk}и традиционно считаются одной из ветвей нигеро-{\cyrchar\cyrk}онголезс{\cyrchar\cyrk}ой ма{\cyrchar\cyrk}росемьи, но при этом ле{\cyrchar\cyrk}си{\cyrchar\cyrk}остатисти{\cyrchar\cyrk}а говорит о том, что дата их разделения составляет не менее 8000 лет. {\cyrchar\CYRP}убли{\cyrchar\cyrk}уемые два {\cyrchar\cyrk}ритичес{\cyrchar\cyrk}их отзыва на статью в определенном смысле явяются ее продолжением, развивая сложную проблемати{\cyrchar\cyrk}у установления дальнего родства язы{\cyrchar\cyrk}ов; авторс{\cyrchar\cyrk}ий ответ на эти отзывы следует та{\cyrchar\cyrk}же рассматривать {\cyrchar\cyrk}а{\cyrchar\cyrk} важный фрагмент публи{\cyrchar\cyrk}уемой статьи.},
  Owner                               = {mattis},
  Timestamp                           = {2015.06.01},
  Userb                               = {On the threshold of relationship and the “stability index” of basic lexicon in mass comparison: Atlantic languages}
}

